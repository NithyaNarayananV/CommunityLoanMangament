        <fullName>State__c</fullName>
        <label>State</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <!-- 
                Open - In General
                Due - Need to be paid in next 30 days
                OverDue - Payment Missed
                Upcoming - Due For rest of the payment (for 10 months)
                Paid On Time - Paid on time : Paid before due date
                Paid Late - Paid after due date : Paid on OverDue Status
                -->
                <value>
                    <fullName>Open</fullName>
                    <default>true</default>
                    <label>Open</label>
                </value>
                <value>
                    <fullName>Due</fullName>
                    <default>false</default>
                    <label>Due</label>
                </value>
                <value>
                    <fullName>OverDue</fullName>
                    <default>false</default>
                    <label>OverDue</label>
                </value>
                <value>
                    <fullName>Upcoming</fullName>
                    <default>false</default>
                    <label>Upcoming</label>
                </value>
                <value>
                    <fullName>Paid On Time</fullName>
                    <default>false</default>
                    <label>Paid On Time</label>
                </value>
                <value>
                    <fullName>Paid Late</fullName>
                    <default>false</default>
                    <label>Paid Late</label>
                </value>
            </valueSetDefinition>
        </valueSet>
